<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <module name="DnD_QuantityUpdater" setup_version="1.0.0">
        <sequence>
            <!-- ensure that modules below will be loaded before my custom module that overrides them -->
            <module name="Magento_Bundle" />
            <module name="Magento_Catalog"/>
            <module name="Magento_Checkout"/>
            <module name="Magento_ConfigurableProduct" />
            <module name="Magento_Downloadable" />
            <module name="Magento_GroupedProduct" />
        </sequence>
    </module>
</config>
